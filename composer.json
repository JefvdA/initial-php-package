{
    "name": "jefvda/initial-php-package",
    "description": "Initial state to build a php-package from",
    "type": "library",
    "version": "0.0.0",
    "authors": [
        {
            "name": "JefvdA"
        }
    ],
    "require": {
        "psr/http-client": "~1.0.0"
    },
    "require-dev": {
        "ergebnis/composer-normalize": "2.42.0",
        "fakerphp/faker": "1.23.1",
        "friendsofphp/php-cs-fixer": "3.58.0",
        "phpstan/phpstan": "1.11.2",
        "phpstan/phpstan-phpunit": "1.4.0",
        "phpunit/phpunit": "11.1.3"
    },
    "autoload": {
        "psr-4": {
            "JefvdA\\InitialPhpPackage\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "JefvdA\\InitialPhpPackage\\Tests\\": "tests/"
        }
    },
    "config": {
        "allow-plugins": {
            "ergebnis/composer-normalize": true
        }
    }
}
